div.seqData {
	width: 100%;
}

div.seqData * {
	cursor: default;
	font-size: 12px;
}

table#tableSeqData {
	border: none;
	float: left;
	width: 100%;
}

table#tableSeqData tr td.seqDataDetsLab {
	width: 85px;
	vertical-align: top;
}

table#tableSeqData tr td {
	padding: 3px;
	border: none;
}

/**	styles infos sequence */
div.clear {
	clear: left;
}

span.infoLab {
	color: blue;
}

span.localCom {
	float: left;
}

div.infos {
	float: left;
}

div.infos div.infosDet {
	visibility: hidden;
	display: none;
	padding: 5px;
	position: absolute;
	background: #F0E68C;
	border: 1px solid black !important;
	z-index: 2;
}

div.infos:hover div.infosDet {
	visibility: visible;
	display: block;
}

/** style zoom */
div#contZoom {
	width: 275px;
	float: left;
}

div#slider {
	float: left;
}

html body div#slider a.ui-slider-handle {
	position: absolute;
	height: 23px;
	width: 12px;
	top: 0px;
	left: 0px;
	/*background:none;
	border:0;*/
	background-image: url(../img/slider-handle.gif);
}

.ui-slider-disabled .ui-slider-handle {
	opacity: 0.5;
	filter: alpha(opacity = 50);
}

.ui-slider {
	width: 200px;
	height: 23px;
	position: relative;
	background-image: url(../img/slider-bg.png);
	background-repeat: no-repeat;
	background-position: center center;
}

div#contCptZoom {
	width: 30px;
	margin-left: 5px;
	margin-top: 3px;
	float: left;
}

